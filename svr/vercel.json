{
    "version": 2,
    "builds": [
      {
        "src": "lapi",
        "use": "@vercel/node"
      },
      {
        "src": "lsocket",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "lapi"
      },
      {
        "src": "/socket.io/(.*)",
        "dest": "lapi"
      }
    ]
  }
  